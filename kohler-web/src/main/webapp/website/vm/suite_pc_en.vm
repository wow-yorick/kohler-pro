<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta http-equiv="Cache-Control" content="max-age=300"/>
    <meta name='keywords' content='$!{data.seo.get(0).seo_keywords}'/>
    <meta name='description' content='$!{data.seo.get(0).seo_description}'/>
    <meta name="applicable-device" content="pc">
   #parse("style_en.vm")
    <title>$!{data.seo.get(0).seo_title}</title>
</head>
<body>
<div class="head_wrapper">
   #parse("header_pc_en.vm")
    <div class="pub_submenu page_menu clearfix">
        <div class="submenu_panel bread actived">
            <div class="bread_panel clearfix">
				#if(${data.breadcrumb_name})
				
				#foreach($breadcrumb_name in ${data.breadcrumb_name})
				
				 <a class="bread_link" href="${breadcrumb_name.url}">$!{breadcrumb_name.name}</a>
				<div class="bread_link" ><img src="${data.base_url}images/arrow2.png" alt=""></div>
				#end
				#end
            </div>
            <div class="favorite2"></div>
            <div class="print"></div>
            <div class="share"></div>
        </div>
    </div>
</div>
<div class="content_wrapper">
<div class="pub_banner product">
    <div class="subwrapper">
       <div class="bgimg actived" style=""><img src="$!{data.hot_spot.get(0).image_url}" style="width:1280px;"></div>
		$!{data.hot_spot.get(0).content}
    </div>
</div>
<div class="product_list collection_details clearfix">
    <div class="right clearfix">
        <h2>$!{data.suite.get(0).name}</h2>
        <div class="search">
            <p>Order</p>
            <ul class="select-panel clearfix">
                <li class="option" value=""><a href="javascript:;">Order1</a></li>
                <li class="option" value=""><a href="javascript:;">Order2</a></li>
                <li class="option" value=""><a href="javascript:;">Order3</a></li>
                <li class="option" value=""><a href="javascript:;">Order4</a></li>
                <li class="option" value=""><a href="javascript:;">Order5</a></li>
                <li class="option" value=""><a href="javascript:;">Order6</a></li>
                <li class="option" value=""><a href="javascript:;">Order7</a></li>
            </ul>
        </div>
        <ul class="item-panel">
      	#if(${data.suite_product})
		 #foreach($suite_product in ${data.suite_product})
			
           <li>
				<a href="$!{suite_product.target_url}"  > 
                <div class="item_img">
                   <img src="$!{suite_product.list_image_url}" alt="$!{suite_product.image_alt}" 	style="width:232px;height:200px;"/>
                </div>
				   <span>
                    <h3>$!{suite_product.collection}</h3>
                    <p>$!{suite_product.product_name}</p>
                    <p class="type">$!{suite_product.sku_code}</p>
                </span>		 
				</a>
            </li>
		
			#end
			#end
           
        </ul>
    </div>
</div>
<div class="pub_totop">
    <div class="totop_btn"><img src="$!{data.base_url}images/totop.png" alt=""></div>
    <span>Back to top</span>
</div>
#parse("footer_pc_en.vm")
</div>
</body>
</html>